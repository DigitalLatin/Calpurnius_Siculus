               <head>Prima Familia</head>
               <!-- Using xml:id for the siglum. -->
               <witness xml:id="N">
                  <msDesc>
                     <msIdentifier>
                        <settlement>Naples</settlement>
                        <repository>Biblioteca Nazionale di Napoli</repository>
                        <idno>V A 8</idno>
